[
        {
            "environment": [
                {
                    "name": "PORT",
                    "value": "80"
                },
                {
                    "name": "BACKEND",
                    "value": "${backend_domain}"
                }
            ],
            "essential": true,
            "memory": 64,
            "image": "${dockerhub_user}/${app}_frontend:latest",
            "name": "frontend",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "${log_group_name}",
                    "awslogs-region": "${log_group_region}"
                }
            }
        }
    ]